/*基础样式*/
body
  background-color:#efeff4
  font-size: 16px
/*内容样式*/
.title
  height: 40px
  padding: 0 12px
  background-color: #fff
  border-bottom: 1px solid #e1e1e1
  text-align: center
  overflow: hidden

.title .nav_back
  display:block
  width: 16px
  height: 16px
  background: url("../../image/nav_back_icon@2x.png")no-repeat
  background-size: 50%
  position absolute
  margin: 12px auto 0
.title .title_text
  display: block
  margin: 12px auto 0
  font-size 20px
.collection_row
  height 134px
  margin 12px
  border 1px solid #e1e1e1
  border-radius 5px
  box-shadow 0 0 2px 1px #e1e1e1
  background-color #fff
  position relative
  .collection_head
    display block
    margin-top 14px
    padding 0 0 14px 12px
    border-bottom 1px dashed #e1e1e1
  .right_text
    font-size 12px
    color #717171
    position absolute
    right 12px
    top 14px
  .collection_text
    font-size 12px
    margin 10px 12px
    line-height 18px
  .collection_link
    display block
    width 90px
    margin 0 auto
    text-align center
    color #717171
    padding 4px 0
    background-color #efeff4
    font-size 14px
    border 1px solid #e1e1e1
    border-radius 4px
